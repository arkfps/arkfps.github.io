---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xmlns:media="http://search.yahoo.com/mrss/" xmlns:webfeeds="http://webfeeds.org/rss/1.0">
  <link href="{{ site.url }}/news/feed-test5" rel="self" type="application/atom+xml"/>
  <link href="{{ site.url }}/" rel="alternate" type="text/html"/>

  <id>{{ site.uuid }}</id>
  <title>{{ site.title | xml_escape }}</title>
  <subtitle>{{ site.description | xml_escape }}</subtitle>
  <category term="{{ site.topic | xml_escape }}" label="{{ site.topic | xml_escape }}"/>
  <author><name>{{ site.creator.full_name | xml_escape }}</name></author>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <rights>Â© {{ site.time | date: '%Y' }} {{ site.fqdn }}</rights>

  <icon>{{ site.url }}/icn/feed/icon.2.svg</icon>
  <webfeeds:icon>{{ site.url }}/icn/feed/icon.2.svg</webfeeds:icon>

  <logo>{{ site.url }}/icn/feed/logo.svg</logo>
  <webfeeds:logo>{{ site.url }}/icn/feed/logo.svg</webfeeds:logo>

  <webfeeds:accentColor>{{ site.colours.main | slice:1,site.colours.main.size }}</webfeeds:accentColor>

  {% for post in site.posts %}
  <entry>
    <link href="{{ post.url | absolute_url }}" rel="alternate" type="text/html" title="{{ post.title | xml_escape }}"/>
    <id>{{ post.uuid }}</id>
    <title>{{ post.title | xml_escape }}</title>
    <category term="{{ site.topic | xml_escape }}" label="{{ site.topic | xml_escape }}"/>
    {% for tag in post.tags %}
    <category term="{{ tag | xml_escape }}" label="{{ tag | xml_escape }}"/>
    {% endfor %}
    <author><name>{{ site.creator.full_name | xml_escape }}</name></author>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.last_modified_at | date_to_xmlschema }}</updated>
    <summary>{{ post.excerpt | strip_footnotes | xml_escape }}</summary>
    <media:thumbnail url="{{ post.image.url | absolute_url }}" width="{{ post.image.url | image_width }}" height="{{ post.image.url | image_height }}"/>
  </entry>
  {% endfor %}
</feed>
