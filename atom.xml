---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{ site.title }}</title>
  <subtitle>{{ site.tagline }}</subtitle>
  <id>{{ site.url }}</id>
  <link href="{{ site.url }}/atom.xml" rel="self"/>
  <link href="{{ site.url }}/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <icon>{{ site.url }}/favicons/favicon.ico</icon>
  <logo>{{ site.url }}/favicons/favicon-192x192.png</logo>
  <rights>Â© {{ site.time | date: '%Y' }} {{ site.author }}.</rights>
  <author>
    <name>{{ site.author }}</name>
  </author>

  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <id>{{ site.url }}/{{ post.id }}</id>
    <link href="{{ site.url }}{{ post.url }}"/>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <summary type="html">{{ post.excerpt | xml_escape }}</summary>
  </entry>
 {% endfor %}
</feed>
