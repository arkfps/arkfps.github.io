{
  "tagname-lowercase": true,
  "attr-lowercase": true,
  "attr-value-double-quotes": false,
  "attr-value-not-empty": false,
  "attr-no-duplication": true,
  "doctype-first": true,
  "tag-pair": true,
  "tag-self-close": false,
  "spec-char-escape": true,
  "id-unique": true,
  "src-not-empty": true,
  "title-require": true,
  "head-script-disabled": true,
  "alt-require": false,
  "doctype-html5": true,
  "id-class-value": true,
  "style-disabled": true,
  "inline-style-disabled": false,
  "inline-script-disabled": false,
  "space-tab-mixed-disabled": true,
  "href-abs-or-rel": false,
  "attr-unsafe-chars": true,
  
  "csslint": {
    
  },
  
  "jshint": {
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "funcscope": false,
    "futurehostile": true,
    "globals": false,
    "iterator": false,
    "latedef": true,
    "maxcomplexity": false,
    "maxdepth": false,
    "maxerr": 50,
    "maxparams": false,
    "maxstatements": false,
    "noarg": true,
    "nocomma": false,
    "nonbsp": true,
    "nonew": true,
    "notypeof": false,
    "predef": ["ga"],
    "shadow": false,
    "singleGroups": false,
    "strict": false,
    "undef": true,
    "unused": false,
    "varstmt": false,
    "asi": true,
    "boss": false,
    "debug": false,
    "elision": false,
    "eqnull": false,
    "evil": false,
    "expr": true,
    "lastsemic": false,
    "loopfunc": false,
    "moz": false,
    "noyield": false,
    "plusplus": false,
    "proto": false,
    "scripturl": false,
    "supernew": false,
    "validthis": false,
    "withstmt": false,
    "browser": true,
    "devel": true,
    "-W018": true,
    "-W058": true
  }
}
